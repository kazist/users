<script>
    jQuery('#country_id').on('change', function () {

        var this_element = jQuery(this);
        var country_id = this_element.val();
        var data_object = {country_id: country_id};

        var form = kaziframework.callAjaxByComponents('setup', 'world', 'world', 'ajaxloadregionoption', 'WorldAjax', data_object, true);

        this_element.closest('form').find('#location_id').html(form);
        //console.log(this_element.closest('form').find('#location_id'));
        return false;
    });
</script>