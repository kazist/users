    "query": {
        "from": {
            "table": "Users\\Accesskeys\\Code\\Tables\\Accesskeys",
            "alias": "ua",
            "name": "#__users_accesskeys"
        },
        "select": [
            "ua.id",
            "ua.accesskey",
            "ua.expiry_date",
            "ua.user_id",
            "uu.name AS user_id_name",
            "ua.date_created",
            "ua.date_modified",
            "ua.modified_by",
            "uu_4390.name AS modified_by_name",
            "ua.created_by",
            "uu_5162.name AS created_by_name"
        ],
        "leftjoin": [
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "ua.user_id = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_4390",
                "name": "#__users_users",
                "on": "ua.modified_by = uu_4390.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_5162",
                "name": "#__users_users",
                "on": "ua.created_by = uu_5162.id"
            }
        ]
    }
